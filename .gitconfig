[core]
	excludesfile = /Users/ryanthames/.gitignore_global
	editor = vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	name = Ryan Thames
	email = ryan.thames@apexcapitalcorp.com
[push]
	default = current
[url "https://"]
	insteadOf = git://
[alias]
	fp = fetch --prune
	s = status
	cm = commit -m
